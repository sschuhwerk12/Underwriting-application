# Server
PORT=3000

# OpenAI
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-5.2-codex

# API security
AI_API_REQUIRE_TOKEN=false
AI_API_TOKEN=change_me_if_enabled

# Rate limiting
AI_RATE_LIMIT_WINDOW_MS=60000
AI_RATE_LIMIT_MAX=30

# Ingestion limits
INGEST_MAX_FILE_BYTES=15728640
INGEST_MAX_FILES=10
INGEST_MAX_TOKENS_PER_CHUNK=1800
INGEST_MAX_TOKENS_PER_BATCH=7000
INGEST_MAX_CHUNKS_PER_BATCH=8
